<div class="row flex">

    <div class="col-7 garages-list ml-3">
      <h1>Voici tous les garages</h1>

        <% @garages.each do |garage| %>

          <%= render 'shared/garagecard', garage: garage %>

        <% end %>

    </div>
    <% markers = [{ lat: 50.706768, lng: 3.222024 }] %>
    <div id="map" class="map col-5 map-box" data-markers="<%= markers.to_json %>"
      data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
    ></div>
</div>

